        "express": "^4.21.1",
        "express-ejs-layouts": "^2.5.1",
        "express-mysql-session": "^3.0.3",
        "express-session": "^1.18.1",
        "glob": "^11.0.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql": "npm:mysql2@^3.11.3",
        "reflect-metadata": "^0.2.2",
        "sass-middleware": "^0.0.3",
        "ts-loader": "^9.5.1",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.20",
        "typeorm-seeding": "^1.6.1",
        "vue": "^3.5.12",
        "vue-router": "^4.4.5",
        "vuex": "^4.1.0",
        "webpack-cli": "^5.1.4",
        "webpack-merge": "^6.0.1",
        "webpack-node-externals": "^3.0.0",
        "winston": "^3.15.0"
    },
