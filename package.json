{
    "name": "codegym",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index.js",
    "scripts": {
        "build": "npm run build:client && npm run build:server",
        "start": "node dist/index.js",
        "dev": "concurrently \"npm run build:client\"  \"npm run build:server\"  \"node dist/server/bundle.server.js\"",
        "test": "echo \"Error: no test specified\" && exit 1",
        "build:client": "webpack --config webpack.client.js --watch",
        "build:server": "webpack --config webpack.server.js --watch",
        "typeorm-seeding": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js",
        "seed:config": "ts-node  -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js config",
        "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
        "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
        "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
